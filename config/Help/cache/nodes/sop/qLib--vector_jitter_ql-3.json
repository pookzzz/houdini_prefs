r"], "extent": [1388, 1413], "body": [{"type": "para", "indent": 12, "text": ["Turns on rotation jitter."], "extent": [1413, 1451]}], "container": true}, {"type": "dt", "indent": 8, "text": ["Angle Amount"], "extent": [1451, 1473], "body": [{"type": "para", "indent": 12, "text": ["Amount of rotation jitter."], "extent": [1473, 1512]}], "container": true}, {"type": "dt", "indent": 8, "text": ["Min Angle"], "extent": [1512, 1531], "body": [{"type": "para", "indent": 12, "text": ["Minimum angle of rotation."], "extent": [1531, 1570]}], "container": true}, {"type": "dt", "indent": 8, "text": ["Max Angle"], "extent": [1570, 1589], "body": [{"type": "para", "indent": 12, "text": ["Maximum angle of rotation."], "extent": [1589, 1628]}], "container": true}, {"type": "dt", "indent": 8, "text": ["Angle Exponent"], "extent": [1628, 1652], "body": [{"type": "para", "indent": 12, "text": ["Distribution exponent of the angle."], "extent": [1652, 1701]}], "container": true}, {"type": "dt", "indent": 8, "text": ["Angle Axis"], "extent": [1701, 1721], "body": [{"type": "para", "indent": 12, "text": ["The axis vector to be used as rotation axis for angular jittering."], "extent": [1721, 1813]}, {"type": "dt_group", "body": [{"type": "dt", "indent": 12, "text": ["Default"], "extent": [1813, 1834], "body": [{"type": "para", "indent": 16, "text": ["The input vector attribute is used."], "extent": [1834, 1886]}], "container": true}, {"type": "dt", "indent": 12, "text": ["Attribute"], "extent": [1886, 1909], "body": [{"type": "para", "indent": 16, "text": ["Another vector attribute can be specified as rotation axis."], "extent": [1909, 1985]}], "container": true}, {"type": "dt", "indent": 12, "text": ["Constant"], "extent": [1985, 2007], "body": [{"type": "para", "indent": 16, "text": ["A constant vector value can be specified as rotation axis."], "extent": [2007, 2083]}], "container": true}], "container": true}, {"type": "note_group", "body": [{"type": "note", "indent": 12, "role": "item", "extent": [2083, 2101], "body": [{"type": "para", "indent": 16, "text": ["All jittered vectors will be ", {"type": "em", "text": ["on the plane defined by this axis"]}, ",\n                resulting in a different look. (experimental in version 3)"], "extent": [2101, 2259]}], "container": true}], "container": true, "role": "item_group"}], "container": true}, {"type": "dt", "indent": 8, "text": ["Seed Offset"], "extent": [2259, 2280], "body": [{"type": "para", "indent": 12, "text": ["Offsets the seed for the angular random function."], "extent": [2280, 2343]}], "container": true}], "container": true}], "container": true}, {"type": "dt", "indent": 4, "text": ["Length"], "extent": [2343, 2355], "body": [{"type": "dt_group", "body": [{"type": "dt", "indent": 8, "text": ["Do Length Jitter"], "extent": [2355, 2381], "body": [{"type": "para", "indent": 12, "text": ["Turns on length jitter."], "extent": [2381, 2417]}], "container": true}, {"type": "dt", "indent": 8, "text": ["Length Amount"], "extent": [2417, 2440], "body": [{"type": "para", "indent": 12, "text": ["Amount of length jitter."], "extent": [2440, 2477]}], "container": true}, {"type": "dt", "indent": 8, "text": ["Min Length Factor"], "extent": [2477, 2504], "body": [{"type": "para", "indent": 12, "text": ["Minimum length factor."], "extent": [2504, 2539]}], "container": true}, {"type": "dt", "indent": 8, "text": ["Max Length Factor"], "extent": [2539, 2566], "body": [{"type": "para", "indent": 12, "text": ["Maximum length factor."], "extent": [2566, 2601]}], "container": true}, {"type": "dt", "indent": 8, "text": ["Length Exponent"], "extent": [2601, 2626], "body": [{"type": "para", "indent": 12, "text": ["Distribution exponent of the length factor."], "extent": [2626, 2682]}], "container": true}, {"type": "dt", "indent": 8, "text": ["Multiplier"], "extent": [2682, 2702], "body": [{"type": "para", "indent": 12, "text": ["An overall multiplier value on top of the jitter."], "extent": [2702, 2764]}], "container": true}, {"type": "dt", "indent": 8, "text": ["Pre-Normalize"], "extent": [2764, 2787], "body": [{"type": "para", "indent": 12, "text": ["Normalize the input vector before applying the jitter."], "extent": [2787, 2854]}], "container": true}, {"type": "dt", "indent": 8, "text": ["Seed Offset"], "extent": [2854, 2875], "body": [{"type": "para", "indent": 12, "text": ["Offsets the seed for the length random function."], "extent": [2875, 2937]}], "container": true}], "container": true}], "container": true}], "container": true}], "text": "Parameters"}, {"level": 1, "id": "related", "container": true, "type": "related_section", "indent": 0, "role": "section", "extent": [2937, 2946], "body": [{"type": "bullet_group", "body": [{"blevel": 6, "type": "bullet", "indent": 4, "text": [{"scheme": "Node", "value": "/nodes/sop/qLib::attr_visualize_ql::1", "type": "link", "text": ["Attr Visualize qL"], "exists": true}], "extent": [2946, 3008]}, {"blevel": 6, "type": "bullet", "indent": 4, "text": [{"scheme": "Node", "value": "/nodes/sop/qLib::attrib_builder_ql::1", "type": "link", "text": ["Attrib Builder qL"], "exists": true}], "extent": [3008, 3070]}, {"blevel": 6, "type": "bullet", "indent": 4, "text": [{"scheme": "Node", "value": "/nodes/sop/qLib::vector_ql::2", "type": "link", "text": ["Vector qL"], "exists": true}], "extent": [3070, 3116]}, {"blevel": 6, "type": "bullet", "indent": 4, "text": [{"scheme": "Node", "value": "/nodes/sop/qLib::vector_store_ql::1", "type": "link", "text": ["Vector Store qL"], "exists": true}], "extent": [3116, 3175]}], "container": true}], "text": "Related"}, {"level": 1, "id": "todo", "container": true, "type": "todo_section", "indent": 0, "text": ["To Do"], "role": "section", "extent": [3175, 3187], "body": [{"type": "bullet_group", "body": [{"blevel": 8, "type": "bullet", "indent": 6, "text": ["\u2026"], "extent": [3187, 3201]}], "container": true}]}, {"level": 1, "id": "relnotes", "container": true, "type": "relnotes_section", "indent": 0, "tex