extent": [3531, 3543], "body": [{"type": "bullet_group", "body": [{"blevel": 6, "type": "bullet", "indent": 4, "text": ["Added ", {"type": "q", "text": ["Transform Attributes"]}, " toggle"], "extent": [3543, 3586]}], "container": true}], "container": true}, {"type": "dt", "indent": 0, "text": ["2014-11-14"], "extent": [3586, 3598], "body": [{"type": "bullet_group", "body": [{"blevel": 6, "type": "bullet", "indent": 4, "text": ["Added ", {"type": "q", "text": ["Show Guide"]}, " toggle"], "extent": [3598, 3631]}], "container": true}], "container": true}, {"type": "dt", "indent": 0, "text": ["2014-03-21"], "extent": [3631, 3643], "body": [{"type": "bullet_group", "body": [{"blevel": 6, "type": "bullet", "indent": 4, "text": ["Minor bug fix in descriptive parm"], "extent": [3643, 3683]}, {"blevel": 6, "type": "bullet", "indent": 4, "text": ["Minor help card enhancement"], "extent": [3683, 3718]}], "container": true}], "container": true}, {"type": "dt", "indent": 0, "text": ["2014-03-19"], "extent": [3718, 3730], "body": [{"type": "bullet_group", "body": [{"blevel": 6, "type": "bullet", "indent": 4, "text": ["Improved node layout info text (descriptive parm)"], "extent": [3730, 3787]}], "container": true}], "container": true}, {"type": "dt", "indent": 0, "text": ["2013-05-09"], "extent": [3787, 3799], "body": [{"type": "bullet_group", "body": [{"blevel": 6, "type": "bullet", "indent": 4, "text": ["Added Parent Object parameter"], "extent": [3799, 3835]}, {"blevel": 6, "type": "bullet", "indent": 4, "text": ["Descriptive parm improvement"], "extent": [3835, 3871]}], "container": true}], "container": true}], "container": true}, {"type": "para", "indent": 0, "text": [{"type": "em", "text": ["older versions"]}, " \u2014"], "extent": [3871, 3893]}, {"type": "dt_group", "body": [{"type": "dt", "indent": 0, "text": ["Version 0.0.1"], "extent": [3893, 3908], "body": [{"type": "para", "indent": 4, "text": ["Initial version (prototype)."], "extent": [3908, 3941]}], "container": true}], "container": true}]}], "title": ["Apply Transform qL"], "summary": ["Applies the full world-space transformation of another object to the input geometry."]}