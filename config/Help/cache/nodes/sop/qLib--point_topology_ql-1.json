y": [{"type": "dt", "indent": 0, "text": ["2017-02-09"], "extent": [9309, 9321], "body": [{"type": "bullet_group", "body": [{"blevel": 6, "type": "bullet", "indent": 4, "text": ["Visualization is not additive by default (#521)"], "extent": [9321, 9375]}, {"blevel": 6, "type": "bullet", "indent": 4, "text": ["Minor update to help for the H15+ help format (handles escape characters differently)"], "extent": [9375, 9468]}], "container": true}], "container": true}, {"type": "dt", "indent": 0, "text": ["2015-01-16"], "extent": [9468, 9480], "body": [{"type": "bullet_group", "body": [{"blevel": 6, "type": "bullet", "indent": 4, "text": ["Fixed parameter back-linking issue with guide group preset menu button"], "extent": [9480, 9558]}], "container": true}], "container": true}, {"type": "dt", "indent": 0, "text": ["2015-01-15"], "extent": [9558, 9570], "body": [{"type": "bullet_group", "body": [{"blevel": 6, "type": "bullet", "indent": 4, "text": ["Point groups (coincident, neighbour) are always created (even if empty)"], "extent": [9570, 9648]}, {"blevel": 6, "type": "bullet", "indent": 4, "text": ["Added group highlighting options (not the prettiest, internally)"], "extent": [9648, 9720]}], "container": true}], "container": true}, {"type": "dt", "indent": 0, "text": ["2014-10-18"], "extent": [9720, 9732], "body": [{"type": "bullet_group", "body": [{"blevel": 6, "type": "bullet", "indent": 4, "text": ["Added ", {"type": "q", "text": ["Velocity Length"]}, " particle option (magnitude of per-point ", {"type": "code", "text": ["v"]}, " attribute)"], "extent": [9732, 9818]}], "container": true}], "container": true}, {"type": "dt", "indent": 0, "text": ["2014-09-15"], "extent": [9818, 9830], "body": [{"type": "bullet_group", "body": [{"blevel": 6, "type": "bullet", "indent": 4, "text": ["Prefixed all internal vopsop params so they wouldn\u2019t collide\n      with regular geometry attributes (e.g. ", {"type": "q", "text": ["radius"]}, ", etc.)"], "extent": [9830, 9958]}, {"blev